# Dakota input file
environment
  tabular_data
    tabular_data_file = 'dakota.dat'

method
  sampling
    sample_type = random
    samples = 100

variables
  uniform_uncertain = 2
    descriptors = 'x1' 'x2'
    lower_bounds = -2.0 -2.0
    upper_bounds = 2.0 2.0

interface
  id_interface = 'CSDMS'
  direct
  analysis_driver = 'rosenbrock'

responses
  response_functions = 1
    response_descriptors = 'y1'
  no_gradients
  no_hessians
